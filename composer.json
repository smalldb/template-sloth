{
    "name": "smalldb/template-sloth",
    "description": "Slot-based template extension",
    "license": "Apache-2.0",
    "type": "symfony-bundle",
    "homepage": "https://smalldb.org/template-sloth/",
    "authors": [
        {
            "name": "Josef Kufner",
            "email": "josef@kufner.cz"
        }
    ],
    "require": {
        "php": ">=7.3",
        "symfony/http-foundation": ">=3.2",
        "twig/twig": ">=3.2"
    },
    "require-dev": {
        "smalldb/doc-tools": "dev-master",
        "phpunit/phpunit": "^9.5",
        "symfony/http-kernel": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Smalldb\\TemplateSloth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Smalldb\\TemplateSloth\\Tests\\": "test/"
        }
    }
}
